# SignalWire Configuration
# Get these from your SignalWire dashboard at https://signalwire.com

# Your SignalWire Space URL (e.g., yourspace.signalwire.com)
export SPACE_URL=your_signalwire_space_url

# Your SignalWire Project ID
export PROJECT_ID=your_signalwire_project_id

# Your SignalWire API Token
export API_TOKEN=your_signalwire_api_token

# Your SignalWire phone number (the number calls will come from)
export FROM_NUMBER=+1234567890

# Application Configuration
# The public URL where SignalWire can reach your webhooks
# For local development, use ngrok or similar tunneling service
export HOST_APP=http://your_public_url:3000

# Port for the Flask server (optional, defaults to 3000)
export PORT=3000

# Usage:
# 1. Copy this file to config.env: cp config.env.example config.env
# 2. Fill in your actual values
# 3. Source the environment: source config.env
# 4. Run the application: python call_hiss.py 